}

class Cliente  {
    - edad : int
    + int getEdad()
    + void setEdad(edad : int)
}

class Vendedor  {
    - direccion : String

    + String getDireccion()
    + void setDirecciont(direccion : String)
}


class Taxi  {
    - color : String
    - patente : String
    - valorPasaje: double

    + String getColor()
    + void setColor(patente : String)
    + String getPatente()
    + void setPatente((patente : String)
    + double getValorPasaje()
    + void setValorPasaje((valor : double)

    + void pagarPasaje (pasaje : int)
}

class Bus{
    - color : String
    - patente : String
    - cantidadAsientos : int

    + String getColor()
    + void setColor(patente : String)
    + String getPatente()
    + void setPatente((patente : String)
    + int getCantidadAsientos()
    + void setCantidadAsientos(asientos : int)

    + int asientosDisponibles()
}

class MiniBus{
    - color : String
    - patente : String
    - tipoViaje : String
    - cantidadAsientos : int

    + String getColor()
    + void setColor(patente : String)
